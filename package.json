    "esprima": "^4.0.0",
    "express": "^4.15.4",
    "extract-zip": "^1.6.0",
    "htmlparser2": "^3.9.2",
    "locate-java-home": "^0.1.6",
    "microtime": "^2.1.6",
    "mitmproxy": "~1.4.0",
    "papaparse": "^4.3.6",
