{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "outDir": "build"
    }
}